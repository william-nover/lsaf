.accordion-header-icon{ background:url(../../assets/images/acc-icn.png) center center no-repeat;float:right;color:transparent;font-size:20px;vertical-align:middle;}
.accordion-header-icon.accordion-header-icon-active{-webkit-transform:rotate(180deg);-ms-transform:rotate(180deg);transform:rotate(180deg);color:transparent;}
