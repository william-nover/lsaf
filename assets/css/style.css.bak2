#menu,
#menu ul,
#menu ul li,
#menu ul li a,
#menu #menu-button {
  margin: 0;
  padding: 0;
  border: 0;
  list-style: none;
  line-height: 1;
  display: block;
  position: relative;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
#menu:after,
#menu > ul:after {
  content: ".";
  display: block;
  clear: both;
  visibility: hidden;
  line-height: 0;
  height: 0;
}
#menu #menu-button {
  display: none;
}
#menu {
	width:599px;
	float: right;
	margin-top:20px;
	z-index:100;
}
#menu > ul > li {
  float: left;
}
#menu.align-center > ul {
  font-size: 0;
  text-align: center;
}
#menu.align-center > ul > li {
  display: inline-block;
  float: none;
}
#menu.align-center ul ul {
  text-align: left;
}
#menu.align-right > ul > li {
  float: right;
}
#menu > ul > li > a {
  padding:13px 12px 18px 12px;
  font-size: 14px;
  text-decoration: none;
  color: #fff;
}
#menu > ul > li:hover > a {
  color: #ffed00;
}
#menu > ul > li.has-sub > a {
}
#menu > ul > li.has-sub > a:after {
  position: absolute;
  top: 22px;
  right: 11px;
  width: 8px;
  height: 2px;
  display: none;
  background: #dddddd;
  content: '';
}
#menu > ul > li.has-sub > a:before {
  position: absolute;
  top: 19px;
  right: 14px;
  display: none;
  width: 2px;
  height: 8px;
  background: #dddddd;
  content: '';
  -webkit-transition: all .40s ease;
  -moz-transition: all .40s ease;
  -ms-transition: all .40s ease;
  -o-transition: all .40s ease;
  transition: all .40s ease;
}
#menu > ul > li.has-sub:hover > a:before {
  top: 23px;
  height: 0;
}
#menu ul ul {
  position: absolute;
  left: -9999px;
}
#menu.align-right ul ul {
  text-align: right;
}
#menu ul ul li {
  height: 0;
  -webkit-transition: all .40s ease;
  -moz-transition: all .40s ease;
  -ms-transition: all .40s ease;
  -o-transition: all .40s ease;
  transition: all .40s ease;
}
#menu li:hover > ul {
  left: auto;
}
#menu.align-right li:hover > ul {
  left: auto;
  right: 0;
}
#menu li:hover > ul > li {
  height: 50px;
}
#menu ul ul ul {
  margin-left: 100%;
  top: 0;
}
#menu.align-right ul ul ul {
  margin-left: 0;
  margin-right: 100%;
}
#menu ul ul li a {
 border-bottom: 1px solid #fff;
  border-top: 1px solid #fff;
  margin-right:2px;
  padding: 12px 15px;
  width: 230px;height: 50px;
  font-size: 13px;
  text-decoration: none;
  color: #dddddd;
  font-weight: 400;
  background:#000; /* For IE8 and earlier */
}

#menu ul ul li:last-child > a,
#menu ul ul li.last-item > a {
  border-bottom: 0;
}
#menu ul ul li:hover > a,
#menu ul ul li a:hover {
  color: #ffffff;
  opacity: 1;
  filter: alpha(opacity=100); /* For IE8 and earlier */
}
#menu ul ul li.has-sub > a:after {
  position: absolute;
  top: 16px;
  right: 11px;
  width: 8px;
  height: 2px;
  display: none;
  background: #dddddd;
  content: '';
}
#menu.align-right ul ul li.has-sub > a:after {
  right: auto;
  left: 11px;
}
#menu ul ul li.has-sub > a:before {
  position: absolute;
  top: 17px;
  right: 14px;
  display: block;
  width: 10px;
  height: 10px;
  background: url(../../assets/images/sub-2.png) center center no-repeat;
  content: '';
  -webkit-transition: all .40s ease;
  -moz-transition: all .40s ease;
  -ms-transition: all .40s ease;
  -o-transition: all .40s ease;
  transition: all .40s ease;
}
#menu.align-right ul ul li.has-sub > a:before {
  right: auto;
  left: 14px;
}
#menu ul ul > li.has-sub:hover > a:before {
  background: url(../../assets/images/sub.png) center center no-repeat;
}

